        <Property Name="Username" Type="Edm.String" MaxLength="40" Nullable="false"/>
        <Property Name="Lastname" Type="Edm.String" MaxLength="60"/>
        <Property Name="Firstname" Type="Edm.String" MaxLength="60"/>
        <Property Name="Email" Type="Edm.String" MaxLength="120"/>
